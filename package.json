{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpeck.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglas-man/pluralsight-js-dev-env.git"
  },
  "author": "Cory House",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/douglas-man/pluralsight-js-dev-env/issues"
  },
  "homepage": "https://github.com/douglas-man/pluralsight-js-dev-env#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^3.10.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.8.3",
    "localtunnel": "^1.8.1",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "npm-run-all": "^3.1.1",
    "nsp": "^2.8.1",
    "numeral": "^1.5.6",
    "open": "0.0.5",
    "path": "^0.12.7",
    "rimraf": "^2.5.4",
    "style-loader": "^0.19.0",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {}
}
